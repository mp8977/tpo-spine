<p id="notice"><%= notice %></p>

<h1>Listing Check Ups</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @check_ups.each do |check_up| %>
      <tr>
        <td><%= check_up.date %></td>
        <td><%= link_to 'Show', check_up %></td>
        <td><%= link_to 'Edit', edit_check_up_path(check_up) %></td>
        <td><%= link_to 'Destroy', check_up, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Check up', new_check_up_path %>
