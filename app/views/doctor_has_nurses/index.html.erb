<p id="notice"><%= notice %></p>

<h1>Listing Doctor Has Nurses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doctor_has_nurses.each do |doctor_has_nurse| %>
      <tr>
        <td><%= link_to 'Show', doctor_has_nurse %></td>
        <td><%= link_to 'Edit', edit_doctor_has_nurse_path(doctor_has_nurse) %></td>
        <td><%= link_to 'Destroy', doctor_has_nurse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

