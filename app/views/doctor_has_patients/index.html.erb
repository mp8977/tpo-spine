<p id="notice"><%= notice %></p>

<h1>Listing Doctor Has Patients</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doctor_has_patients.each do |doctor_has_patient| %>
      <tr>
        <td><%= link_to 'Show', doctor_has_patient %></td>
        <td><%= link_to 'Edit', edit_doctor_has_patient_path(doctor_has_patient) %></td>
        <td><%= link_to 'Destroy', doctor_has_patient, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

