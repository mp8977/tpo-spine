<p id="notice"><%= notice %></p>

<h1>Listing Measurement Docs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @measurement_docs.each do |measurement_doc| %>
      <tr>
        <td><%= link_to 'Show', measurement_doc %></td>
        <td><%= link_to 'Edit', edit_measurement_doc_path(measurement_doc) %></td>
        <td><%= link_to 'Destroy', measurement_doc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Measurement doc', new_measurement_doc_path %>
