<p id="notice"><%= notice %></p>

<h1>Listing Measurement Homes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @measurement_homes.each do |measurement_home| %>
      <tr>
        <td><%= link_to 'Show', measurement_home %></td>
        <td><%= link_to 'Edit', edit_measurement_home_path(measurement_home) %></td>
        <td><%= link_to 'Destroy', measurement_home, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Measurement home', new_measurement_home_path %>
