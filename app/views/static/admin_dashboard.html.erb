<h2>Adminstratorjev Dashboard</h2>
<br>
<div class="container">
  <div class="row profile">
    <div class="col-md-3">
      <div class="profile-sidebar">
        <!-- SIDEBAR USER TITLE -->
        <div class="profile-usertitle">
          <div class="profile-usertitle-name">
            <%= current_admin.email %>
            <br>
            <%= current_admin.last_sign_in_at %>
          </div>
        </div>
        <!-- END SIDEBAR USER TITLE -->
        <!-- SIDEBAR BUTTONS -->
        <div class="profile-userbuttons">
          <%=link_to 'Sign_out', destroy_admin_session_path, :method => :delete%>
        </div>
        <!-- END SIDEBAR BUTTONS -->
        <!-- SIDEBAR MENU -->
        <div class="profile-usermenu">
          <ul class="nav">
            <li class="active">
              <a href="/doctors/sign_up">
                <i class="glyphicon glyphicon-user"></i>
                Registriraj zdravnika</a>
            </li>
            <li>
              <a href="/nurses/sign_up">
                <i class="glyphicon glyphicon-user"></i>
                Registriraj med. sestro </a>
            </li>
          </ul>
        </div>
        <!-- END MENU -->
      </div>
    </div>
    <div class="col-md-9">
      <div class="profile-content">
        <%= yield %>
      </div>
    </div>
  </div>
</div>
<br>
<br>